+    "using Luxor, Colors\n",
+    "@draw begin\n",
+    "    # draw book\n",
+    "    Drawing(800, 600, \"sciml-book-logo.svg\")\n",
+    "    origin()\n",
+    "    background(\"transparent\")\n",
+    "    const sciml_blue = \"#213055\"\n",
+    "    🐢 = Turtle()\n",
+    "    Pencolor(🐢, sciml_blue)\n",
+    "    Penwidth(🐢, 4)\n",
+    "    squircle(Point(0,0), 140, 200; action = :fill, rt = 0.1)\n",
+    "    squircle(Point(10,10), 140, 200; action = :stroke, rt = 0.1)\n",
