+    "    placeimage(sciml, Point(-120, -120))\n",
+    "    Pencolor(🐢, \"#fff\")\n",
+    "    fontsize(42)\n",
+    "    fontface(\"JuliaMono\")\n",
+    "    text(\"SciML Book\", Point(0, -160), halign=:center, valign=:center)\n",
